[diff]
    tool = kdiff3
[filter "lfs"]
	smudge = git-lfs smudge %f
	required = true
	clean = git-lfs clean %f
[credential]
	helper = wincred
[auto]
	crlf = input
[core]
	autocrlf = input
[color]
	ui = true
[difftool]
	prompt = false
[mergetool]
	prompt = false
[push]
	default = simple
[alias]
	lol = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
